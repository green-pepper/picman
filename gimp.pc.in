prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
datarootdir=@datarootdir@

picmandatadir=@picmandatadir@
picmanlibdir=@picmanplugindir@
picmansysconfdir=@picmansysconfdir@
picmanlocaledir=@picmanlocaledir@

Name: PICMAN
Description: PICMAN Library
Version: @PICMAN_REAL_VERSION@
Requires: gdk-pixbuf-2.0 >= @GDK_PIXBUF_REQUIRED_VERSION@ cairo >= @CAIRO_REQUIRED_VERSION@ gegl-0.2 >= @GEGL_REQUIRED_VERSION@
Libs: -L${libdir} -lpicman-@PICMAN_API_VERSION@ -lpicmanmath-@PICMAN_API_VERSION@  -lpicmanconfig-@PICMAN_API_VERSION@ -lpicmancolor-@PICMAN_API_VERSION@ -lpicmanbase-@PICMAN_API_VERSION@ @RT_LIBS@
Cflags: -I${includedir}/picman-@PICMAN_API_VERSION@
