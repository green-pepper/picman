msgid "Do not show a splash screen"
msgstr "Nezobrazí úvodné okno"

# popis voľby príkazového riadka
#: ../app/main.c:193
msgid "Do not use shared memory between PICMAN and plugins"
msgstr "Nepoužije zdieľanú pamäť medzi programom PICMAN a zásuvnými modulmi"

# popis voľby príkazového riadka
#: ../app/main.c:198
msgid "Do not use special CPU acceleration functions"
msgstr "Nepoužije špeciálne funkcie procesora na urýchľovanie"

